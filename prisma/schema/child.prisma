// 자식 컴포넌트
model Child {
    id     Int     @id @default(autoincrement())
    image  String?
    width  String?
    height String?
    margin String?

    // 삭제 여부
    isDelete Boolean @default(false)

    // 부모 컴포넌트
    componentId Int
    component   Component @relation(fields: [componentId], references: [id])
}
